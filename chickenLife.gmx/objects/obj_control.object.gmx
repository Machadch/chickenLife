global.osPause = 0;
    global.loadDiff = date_second_span(date_current_datetime(),global.savedTime);
    if (global.loadDiff &gt; 2 ) {
        pauseHunger = ((global.loadDiff * 30) * obj_chicken.hungerDegrade);
        pauseThirst = ((global.loadDiff * 30) * obj_chicken.thirstDegrade);
        //Deal with hunger
            if (obj_chicken.hunger &lt; pauseHunger) {
            obj_chicken.hunger = 0;
            roundHunger = (pauseHunger / 100);
            round(roundHunger);
            if (obj_feeder.feedLevel &gt;= roundHunger) {
                obj_feeder.feedLevel -= roundHunger;            
                }
            else obj_feeder.feedLevel = 0;
            }
        else obj_chicken.hunger -= pauseHunger;
        //Deal with thirst
        if (obj_chicken.thirst &lt; pauseThirst) {
            obj_chicken.thirst = 0;
            roundThirst = (pauseThirst / 100);
            round(roundThirst);
            if (obj_water.waterLevel &gt;= roundThirst) {
                obj_water.waterLevel -= roundThirst;            
                }
            else obj_water.waterLevel = 0;
            }
        else obj_chicken.thirst -= pauseThirst;
        global.loadDiff = 0;
        global.osPause = 0;
        obj_chicken.hungerDegrade = tmpHunger;
        obj_chicken.thirstDegrade = tmpThirst;
        }
}
